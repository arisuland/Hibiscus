{
  "name": "@monori/frontend",
  "version": "1.0.0",
  "private": true,
  "description": "ðŸ‘»ðŸ’» Frontend portion for Monori, made with React using TailwindCSS",
  "author": "Chris \"August\" Hernandez <august@augu.dev>",
  "license": "MIT",
  "repository": "https://github.com/monori-site/frontend",
  "scripts": {
    "build:css:watch": "yarn postcss src/styles/**/*.scss --dir src/styles/ --ext css --verbose --watch && yarn cleanup",
    "build:css": "yarn postcss src/styles/**/*.scss --dir src/styles/ --ext css --verbose && yarn cleanup",
    "cleanup": "rm -fr node_modules/@types/**/node_modules && rm src/styles/**.css.map",
    "start": "yarn react-scripts start",
    "build": "yarn react-scripts build",
    "e2e": "cypress open"
  },
  "dependencies": {
    "@augu/orchid": "1.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "tailwindcss": "1.8.4"
  },
  "devDependencies": {
    "@augu/eslint-config": "1.7.3",
    "@csstools/postcss-sass": "4.0.0",
    "@types/node": "14.6.4",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "autoprefixer": "9.8.6",
    "eslint": "7.8.1",
    "postcss": "7.0.32",
    "postcss-cli": "7.1.2",
    "postcss-import": "12.0.1",
    "typescript": "4.0.2"
  },
  "browserslist": {
